<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WeaponRental.WPF.Styles">
    <!--Colors-->
    <Color x:Key="ColorPrimary1">#3d8abe</Color>
    <Color x:Key="ColorPrimary2">#16aee0</Color>
    <Color x:Key="ColorMenu1">#212b34</Color>
    <Color x:Key="ColorMenu2">#192026</Color>
    <Color x:Key="ColorBackground">#ededf5</Color>
    
    <!--Brushes-->
    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource ColorPrimary1}"/>
    <SolidColorBrush x:Key="BrushPrimary2" Color="{StaticResource ColorPrimary2}"/>
    <SolidColorBrush x:Key="BrushMenu1" Color="{StaticResource ColorMenu1}"/>
    <SolidColorBrush x:Key="BrushMenu2" Color="{StaticResource ColorMenu2}"/>
    <SolidColorBrush x:Key="BrushBackground" Color="{StaticResource ColorBackground}"/>

</ResourceDictionary>